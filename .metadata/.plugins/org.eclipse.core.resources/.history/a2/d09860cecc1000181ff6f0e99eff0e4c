package entities;

import javax.persistence.Entity;
import javax.persistence.Table;

@Entity
@Table(name = "service_offer_location")
public class ServiceOfferLocation {

	private int id;
	
	private ServiceOffer serviceOfferLocationParent;
	
	private GenericLocation serviceOfferGenericLocation;
	
	private Address serviceOfferAddress;
	
	private String title;
	
	private String description;
	
	private Boolean publicVisibility;
	
	
}
